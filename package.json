{
    "name": "EeseExec",
    "displayName": "EeseExec",
    "description": "",
    "version": "0.0.1",
    "publisher": "njmccann",
    "engines": {
        "vscode": "^0.10.6"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.execSelected"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.execSelected",
                "title": "EeseExec: Execute selection"
            }
        ],
        "keybindings": [
            {
                "command": "extension.execSelected",
                "key": "f4",
                "mac": "f4",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}